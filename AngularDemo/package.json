{
  "name": "angular-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "npm-i": "npm i --legacy-peer-deps",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --base-href /angularDemo --deploy-url /angularDemo/ --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "eslint": "eslint \"./src/*.ts\" \"./src/**/*.ts\" --fix",
    "prettier": "prettier \"./src/*.ts\" \"./src/**/*.ts\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier \"./src/*.ts\" \"./src/**/*.ts\" --write",
      "eslint \"./src/*.ts\" \"./src/**/*.ts\" --fix"
    ]
  },
  "private": true,
